{
  "name": "htn252",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "landing",
    "donor-dashboard",
    "store-dashboard",
    "NGO-dashboard",
    "payment-terminal",
    "user-creation-terminal",
    "master-landing"
  ],
  "scripts": {
    "dev:landing": "npm run dev --workspace landing-page",
    "dev:donor": "npm run dev --workspace donor-dashboard",
    "dev:store": "npm run dev --workspace store-dashboard",
    "dev:ngo": "npm run dev --workspace ngo-dashboard",
    "dev:terminal": "npm run dev --workspace payment-terminal",
    "dev:user": "npm run dev --workspace user-creation-terminal",
    "dev:master": "npm run dev --workspace master-landing",
    "dev:all": "concurrently \"npm run dev --workspace landing-page\" \"npm run dev --workspace master-landing\" \"npm run dev --workspace donor-dashboard\" \"npm run dev --workspace store-dashboard\" \"npm run dev --workspace ngo-dashboard\" \"npm run dev --workspace payment-terminal\" \"npm run dev --workspace user-creation-terminal\""
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "vite": "^6.3.6"
  }
}
